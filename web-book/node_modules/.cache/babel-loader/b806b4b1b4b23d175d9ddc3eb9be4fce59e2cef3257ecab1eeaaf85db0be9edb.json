 <el-table v-else>\r\n          <!-- 表格列定义... -->\r\n        </el-table>\r\n      </el-card>\r\n\r\n      <!-- 收货信息卡片 -->\r\n      <el-card class=\"address-card animated fadeInUp delay-1\">\r\n        <div slot=\"header\" class=\"card-header-decor\">\r\n          <span class=\"decor-line\"></span>\r\n          <h3 class=\"card-title\">\r\n            <i class=\"el-icon-location-information\"></i>\r\n            收货信息\r\n          </h3>\r\n          <span class=\"decor-line\"></span>\r\n        </div>\r\n\r\n        <!-- 动态表单验证 -->\r\n        <el-form class=\"floating-label-form\">\r\n          <!-- 表单项目... -->\r\n        </el-form>\r\n      </el-card>\r\n\r\n      <!-- 价格汇总面板 -->\r\n      <div class=\"summary-panel animated fadeInUp delay-2\">\r\n        <div class=\"price-breakdown\">\r\n          <div class=\"price-item\">\r\n            <span>商品总价</span>\r\n            <span class=\"value\">¥{{ total.toFixed(2) }}</span>\r\n          </div>\r\n          <div class=\"price-item discount\">\r\n            <span>优惠减免</span>\r\n            <span class=\"value\">-¥0.00</span>\r\n          </div>\r\n          <div class=\"price-item total\">\r\n            <span>应付总额</span>\r\n            <span class=\"value\">¥{{ total.toFixed(2) }}</span>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 3D效果按钮 -->\r\n        <el-button\r\n            class=\"submit-btn neumorphic\"\r\n            @click=\"handleSubmit\">\r\n          <i class=\"el-icon-wallet\"></i>\r\n          立即支付\r\n        </el-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    // 手机号验证规则\r\n    const validatePhone = (rule, value, callback) => {\r\n      if (!/^1[3-9]\\d{9}$/.test(value)) {\r\n        callback(new Error('请输入正确的手机号码'));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n\r\n    return {\r\n      confirmData: null,\r\n      form: {\r\n        address: '',\r\n        recipient: '',\r\n        tel: ''\r\n      },\r\n      rules: {\r\n        address: [\r\n          { required: true, message: '请输入收货地址', trigger: 'blur' }\r\n        ],\r\n        recipient: [\r\n          { required: true, message: '请输入收件人姓名', trigger: 'blur' }\r\n        ],\r\n        tel: [\r\n          { required: true, message: '请输入联系方式', trigger: 'blur' },\r\n          { validator: validatePhone, trigger: 'blur' }\r\n        ]\r\n      },\r\n      total: 0\r\n    };\r\n  },\r\n  created() {\r\n    this.loadConfirmData();\r\n  },\r\n  methods: {\r\n    async loadConfirmData() {\r\n      try {\r\n        const oiid = this.$route.query.oiid;\r\n        const { data } = await this.$axios.get(\r\n            `${this.$httpUrl}/order/confirm?oiid=${oiid}`\r\n        );\r\n\r\n        if (data.code === 200) {\r\n          this.confirmData = data.data;\r\n          this.total = data.data.discountPrice * data.data.orderItem.quantity;\r\n        }\r\n      } catch (error) {\r\n        this.$message.error('获取订单信息失败');\r\n      }\r\n    },\r\n\r\n    handleSubmit() {\r\n      this.$refs.formRef.validate(async valid => {\r\n        if (valid) {\r\n          try {\r\n            const { data } = await this.$axios.post(\r\n                `${this.$httpUrl}/order/create`,\r\n                {\r\n                  oiid: this.$route.query.oiid,\r\n                  ...this.form\r\n                }\r\n            );\r\n\r\n            if (data.code === 200) {\r\n              this.$router.push({\r\n                path: '/pay',\r\n                query: {\r\n                  oid: data.data.oid,\r\n                  total: data.data.total\r\n                }\r\n              });\r\n            }\r\n          } catch (error) {\r\n            this.$message.error('提交订单失败');\r\n          }\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 渐变背景 */\r\n.confirm-container {\r\n  position: relative;\r\n  min-height: 100vh;\r\n  background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%);\r\n}\r\n\r\n.gradient-bg {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  height: 300px;\r\n  background: linear-gradient(45deg, #6c5ce7, #a66efa);\r\n  clip-path: polygon(0 0, 100% 0, 100% 80%, 0 100%);\r\n}\r\n\r\n/* 主卡片容器 */\r\n.main-card {\r\n  position: relative;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 40px 30px;\r\n  z-index: 1;\r\n}\r\n\r\n/* 进度步骤 */\r\n.progress-steps {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.step {\r\n  flex: 1;\r\n  text-align: center;\r\n  padding: 15px;\r\n  background: rgba(255,255,255,0.9);\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 6px rgba(0,0,0,0.05);\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.step.active {\r\n  background: #fff;\r\n  box-shadow: 0 8px 25px rgba(108,92,231,0.15);\r\n  transform: translateY(-5px);\r\n}\r\n\r\n/* 卡片装饰 */\r\n.card-header-decor {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.decor-line {\r\n  flex: 1;\r\n  height: 2px;\r\n  background: linear-gradient(90deg, transparent, #6c5ce7, transparent);\r\n}\r\n\r\n.card-title {\r\n  margin: 0 20px;\r\n  color: #6c5ce7;\r\n  font-weight: 600;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n/* 骨架屏加载动画 */\r\n@keyframes shimmer {\r\n  100% { background-position: 100% 0; }\r\n}\r\n\r\n.skeleton-loading {\r\n  display: flex;\r\n  padding: 20px;\r\n}\r\n\r\n.skeleton-image {\r\n  width: 100px;\r\n  height: 140px;\r\n  background: linear-gradient(90deg, #f0f0f0 25%, #e0e0e0 50%, #f0f0f0 75%);\r\n  background-size: 400% 100%;\r\n  animation: shimmer 1.5s infinite linear;\r\n  border-radius: 8px;\r\n}\r\n\r\n.skeleton-content {\r\n  flex: 1;\r\n  margin-left: 20px;\r\n}\r\n\r\n.skeleton-line {\r\n  height: 16px;\r\n  background: #f0f0f0;\r\n  margin-bottom: 10px;\r\n  border-radius: 4px;\r\n  background: linear-gradient(90deg, #f0f0f0 25%, #e0e0e0 50%, #f0f0f0 75%);\r\n  background-size: 400% 100%;\r\n  animation: shimmer 1.5s infinite linear;\r\n}\r\n\r\n/* 价格汇总面板 */\r\n.summary-panel {\r\n  background: #fff;\r\n  border-radius: 15px;\r\n  padding: 30px;\r\n  box-shadow: 0 10px 30px rgba(0,0,0,0.08);\r\n  margin-top: 30px;\r\n}\r\n\r\n.price-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin-bottom: 15px;\r\n  padding: 12px 0;\r\n  border-bottom: 1px solid #eee;\r\n}\r\n\r\n.price-item.total {\r\n  border-bottom: none;\r\n  font-size: 18px;\r\n  font-weight: 600;\r\n  color: #6c5ce7;\r\n}\r\n\r\n/* 新拟态按钮 */\r\n.neumorphic {\r\n  background: #f0f0f3;\r\n  border: none;\r\n  border-radius: 15px;\r\n  box-shadow: -10px -10px 20px #ffffff,\r\n  10px 10px 20px rgba(174,174,174,0.2);\r\n  transition: all 0.2s ease;\r\n}\r\n\r\n.neumorphic:hover {\r\n  box-shadow: -5px -5px 10px #ffffff,\r\n  5px 5px 10px rgba(174,174,174,0.2);\r\n}\r\n\r\n/* 动画效果 */\r\n@keyframes fadeIn {\r\n  from { opacity: 0; transform: translateY(20px); }\r\n  to { opacity: 1; transform: translateY(0); }\r\n}\r\n\r\n.animated {\r\n  animation-duration: 0.6s;\r\n  animation-fill-mode: both;\r\n}\r\n\r\n.fadeIn { animation-name: fadeIn; }\r\n.fadeInUp { animation-name: fadeInUp; }\r\n\r\n.delay-1 { animation-delay: 0.2s; }\r\n.delay-2 { animation-delay: 0.4s; }\r\n\r\n@media (max-width: 768px) {\r\n  .main-card {\r\n    padding: 20px 15px;\r\n  }\r\n\r\n  .progress-steps {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .step {\r\n    margin-bottom: 15px;\r\n  }\r\n}\r\n</style>"],"mappings":";AAyFA;EACAA,KAAA;IACA;IACA,MAAAC,aAAA,GAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,qBAAAC,IAAA,CAAAF,KAAA;QACAC,QAAA,KAAAE,KAAA;MACA;QACAF,QAAA;MACA;IACA;IAEA;MACAG,WAAA;MACAC,IAAA;QACAC,OAAA;QACAC,SAAA;QACAC,GAAA;MACA;MACAC,KAAA;QACAH,OAAA,GACA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAL,SAAA,GACA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAJ,GAAA,GACA;UAAAE,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,SAAA,EAAAf,aAAA;UAAAc,OAAA;QAAA;MAEA;MACAE,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,eAAA;EAC