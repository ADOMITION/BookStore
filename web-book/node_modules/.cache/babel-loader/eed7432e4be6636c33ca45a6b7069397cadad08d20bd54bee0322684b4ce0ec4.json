{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"SearchPage\",\n  data() {\n    return {\n      beans: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 12\n    };\n  },\n  created() {\n    this.refresh();\n  },\n  watch: {\n    '$route'() {\n      this.refresh();\n    }\n  },\n  methods: {\n    async refresh() {\n      const params = {\n        pageNum: this.pageNum,\n        pageSize: this.pageSize,\n        param: {\n          keyword: this.$route.query.keyword\n        }\n      };\n      try {\n        const res = (await this.$axios.post(this.$httpUrl + '/book/search', params)).data;\n        this.beans = res.data.records;\n        this.total = res.data.total;\n        console.log(res);\n      } catch (error) {\n        this.$message.error('搜索失败，请稍后重试');\n      }\n    },\n    handlePageChange(page) {\n      this.pageNum = page;\n      this.refresh();\n    },\n    handleSizeChange(size) {\n      this.pageSize = size;\n      this.refresh();\n    },\n    handleSkip(id) {\n      this.$router.push({\n        path: '/book',\n        query: {\n          id\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","beans","total","pageNum","pageSize","created","refresh","watch","$route","methods","params","param","keyword","query","res","$axios","post","$httpUrl","records","console","log","error","$message","handlePageChange","page","handleSizeChange","size","handleSkip","id","$router","push","path"],"sources":["src/user/home/SearchPage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <!--未找到结果-->\r\n      <el-alert\r\n          v-if=\"beans === ''\"\r\n          title=\"没有相关结果哦，换个关键字如何？\"\r\n          type=\"primary\"\r\n      >\r\n      </el-alert>\r\n      <!--找到结果-->\r\n      <div class=\"result-grid\">\r\n        <el-row :gutter=\"30\">\r\n          <el-col\r\n              v-for=\"item in beans\"\r\n              :key=\"item.id\"\r\n              :xs=\"12\" :sm=\"8\" :md=\"6\" :lg=\"4\"\r\n              class=\"result-item\"\r\n          >\r\n            <el-card\r\n                class=\"book-card\"\r\n                shadow=\"hover\"\r\n                @click.native=\"handleSkip(item.id)\"\r\n            >\r\n              <el-image\r\n                  :src=\"item.image\"\r\n                  class=\"book-cover\"\r\n                  fit=\"cover\"\r\n              >\r\n                <div slot=\"error\" class=\"image-placeholder\">\r\n                  <i class=\"el-icon-picture-outline\"></i>\r\n                </div>\r\n              </el-image>\r\n\r\n              <div class=\"book-info\">\r\n                <p class=\"book-title\">{{ item.name }}</p>\r\n                <p class=\"book-author\">{{ item.author }}</p>\r\n\r\n                <div class=\"meta-info\">\r\n                  <div class=\"price-rating\">\r\n                    <span class=\"price\">¥{{ item.price.toFixed(2) }}</span>\r\n                    <el-rate\r\n                        v-model=\"item.mark\"\r\n                        :max=\"10\"\r\n                        disabled\r\n                        class=\"search-rating\"\r\n                    />\r\n                  </div>\r\n                  <p class=\"book-press\">{{ item.press }}</p>\r\n                </div>\r\n              </div>\r\n            </el-card>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n    </div>\r\n    <!-- 分页组件 -->\r\n    <el-pagination\r\n        v-if=\"total > 0\"\r\n        background\r\n        layout=\"prev, pager, next, jumper, sizes, total\"\r\n        :page-sizes=\"[12, 24, 48, 96]\"\r\n        :current-page=\"pageNum\"\r\n        :page-size=\"pageSize\"\r\n        :total=\"total\"\r\n        @current-change=\"handlePageChange\"\r\n        @size-change=\"handleSizeChange\"\r\n        class=\"pagination\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"SearchPage\",\r\n  data() {\r\n    return {\r\n      beans: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 12,\r\n    }\r\n  },\r\n  created() {\r\n    this.refresh();\r\n  },\r\n  watch: {\r\n    '$route'() {\r\n      this.refresh();\r\n    }\r\n  },\r\n  methods: {\r\n    async refresh() {\r\n      const params = {\r\n        pageNum: this.pageNum,\r\n        pageSize: this.pageSize,\r\n        param: {\r\n          keyword: this.$route.query.keyword\r\n        }\r\n      };\r\n\r\n      try {\r\n        const res = (await this.$axios.post(this.$httpUrl + '/book/search', params)).data;\r\n        this.beans = res.data.records;\r\n        this.total = res.data.total;\r\n        console.log(res)\r\n      } catch (error) {\r\n        this.$message.error('搜索失败，请稍后重试');\r\n      }\r\n    },\r\n    handlePageChange(page) {\r\n      this.pageNum = page;\r\n      this.refresh();\r\n    },\r\n    handleSizeChange(size) {\r\n      this.pageSize = size;\r\n      this.refresh();\r\n    },\r\n\r\n    handleSkip(id) {\r\n      this.$router.push({\r\n        path: '/book',\r\n        query: { id }\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AAyEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,KAAA;IACA,QAAAC,CAAA;MACA,KAAAF,OAAA;IACA;EACA;EACAG,OAAA;IACA,MAAAH,QAAA;MACA,MAAAI,MAAA;QACAP,OAAA,OAAAA,OAAA;QACAC,QAAA,OAAAA,QAAA;QACAO,KAAA;UACAC,OAAA,OAAAJ,MAAA,CAAAK,KAAA,CAAAD;QACA;MACA;MAEA;QACA,MAAAE,GAAA,eAAAC,MAAA,CAAAC,IAAA,MAAAC,QAAA,mBAAAP,MAAA,GAAAV,IAAA;QACA,KAAAC,KAAA,GAAAa,GAAA,CAAAd,IAAA,CAAAkB,OAAA;QACA,KAAAhB,KAAA,GAAAY,GAAA,CAAAd,IAAA,CAAAE,KAAA;QACAiB,OAAA,CAAAC,GAAA,CAAAN,GAAA;MACA,SAAAO,KAAA;QACA,KAAAC,QAAA,CAAAD,KAAA;MACA;IACA;IACAE,iBAAAC,IAAA;MACA,KAAArB,OAAA,GAAAqB,IAAA;MACA,KAAAlB,OAAA;IACA;IACAmB,iBAAAC,IAAA;MACA,KAAAtB,QAAA,GAAAsB,IAAA;MACA,KAAApB,OAAA;IACA;IAEAqB,WAAAC,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QACAC,IAAA;QACAlB,KAAA;UAAAe;QAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"met