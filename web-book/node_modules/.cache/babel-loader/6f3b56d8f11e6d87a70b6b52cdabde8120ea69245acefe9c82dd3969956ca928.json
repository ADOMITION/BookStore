{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"SearchPage\",\n  data() {\n    return {\n      loading: true,\n      beans: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 12\n    };\n  },\n  created() {\n    this.refresh();\n  },\n  watch: {\n    '$route'() {\n      this.refresh();\n    }\n  },\n  methods: {\n    async refresh() {\n      this.loading = true;\n      try {\n        const res = await this.$axios.post(this.$httpUrl + '/book/search', {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          param: {\n            keyword: this.$route.query.keyword\n          }\n        });\n        if (res.data.code === 200) {\n          this.beans = res.data?.data || [];\n          this.total = res.data?.total || 0;\n          console.log(res);\n          console.log(res.data.data);\n        }\n      } catch (error) {\n        this.$message.error('搜索失败，请稍后重试');\n      } finally {\n        this.loading = false;\n      }\n    },\n    handlePageChange(page) {\n      this.pageNum = page;\n      this.refresh();\n    },\n    handleSizeChange(size) {\n      this.pageSize = size;\n      this.refresh();\n    },\n    handleSkip(id) {\n      this.$router.push({\n        path: '/book',\n        query: {\n          id\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","loading","beans","total","pageNum","pageSize","created","refresh","watch","$route","methods","res","$axios","post","$httpUrl","param","keyword","query","code","console","log","error","$message","handlePageChange","page","handleSizeChange","size","handleSkip","id","$router","push","path"],"sources":["src/user/home/SearchPage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div v-loading=\"loading\">\r\n      <!--未找到结果-->\r\n      <el-alert\r\n          v-if=\"!loading && beans.length === 0  \"\r\n          title=\"没有相关结果哦，换个关键字如何？\"\r\n          type=\"info\"\r\n          :closable=\"false\"\r\n      />\r\n\r\n      <!--找到结果-->\r\n      <div v-else>\r\n        <div class=\"result-grid\">\r\n          <el-row :gutter=\"30\">\r\n            <el-col\r\n                v-for=\"item in beans\"\r\n                :key=\"item.id\"\r\n                :xs=\"12\" :sm=\"8\" :md=\"6\" :lg=\"4\"\r\n                style=\"margin-bottom: 20px; padding: 0 15px\"\r\n            >\r\n\r\n              <el-card\r\n                  :body-style=\"{padding:'0px'}\"\r\n                  style=\"width: 195px; cursor: pointer;\"\r\n                  shadow=\"hover\"\r\n                  @click.native=\"handleSkip(item.id)\">\r\n\r\n                <el-image\r\n                    :src=\"item.image\"\r\n                    style=\"height: 276px;width: 195px\"\r\n                    fit=\"cover\">\r\n                  <div slot=\"error\" class=\"image-placeholder\">\r\n                    <i class=\"el-icon-picture-outline\"></i>\r\n                  </div>\r\n                </el-image>\r\n\r\n                <div style=\"padding: 10px; margin-top: -10px\">\r\n                  <p class=\"book-title\">{{ item.name }}</p>\r\n                  <p class=\"book-author\">{{ item.author }}</p>\r\n                  <p class=\"book-press\">{{ item.press }}</p>\r\n\r\n                  <!-- 评分 -->\r\n                  <div class=\"price-rating\">\r\n                    <span class=\"price\">¥{{ item.price }}</span>\r\n                    <div class=\"rating-box\">\r\n                      评分：{{ item.mark || '暂无' }}\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </el-card>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n    </div>\r\n      <!-- 分页 -->\r\n      <el-pagination\r\n          v-if=\"total > 0\"\r\n          background\r\n          layout=\"prev, pager, next, jumper, sizes, total\"\r\n          :page-sizes=\"[12, 24, 48, 96]\"\r\n          :current-page=\"pageNum\"\r\n          :page-size=\"pageSize\"\r\n          :total=\"total\"\r\n          @current-change=\"handlePageChange\"\r\n          @size-change=\"handleSizeChange\"\r\n          class=\"pagination\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"SearchPage\",\r\n  data() {\r\n    return {\r\n      loading : true,\r\n      beans: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 12,\r\n    }\r\n  },\r\n  created() {\r\n    this.refresh();\r\n  },\r\n  watch: {\r\n    '$route'() {\r\n      this.refresh();\r\n    }\r\n  },\r\n  methods: {\r\n    async refresh() {\r\n      this.loading = true;\r\n      try {\r\n        const res = (await this.$axios.post(this.$httpUrl + '/book/search', {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          param: {\r\n            keyword: this.$route.query.keyword\r\n          }\r\n        }));\r\n        if (res.data.code === 200) {\r\n          this.beans = res.data?.data || [];\r\n          this.total = res.data?.total || 0;\r\n          console.log(res)\r\n          console.log(res.data.data)\r\n        }\r\n      } catch (error) {\r\n        this.$message.error('搜索失败，请稍后重试');\r\n      }finally {\r\n        this.loading =false\r\n      }\r\n    },\r\n    handlePageChange(page) {\r\n      this.pageNum = page;\r\n      this.refresh();\r\n    },\r\n    handleSizeChange(size) {\r\n      this.pageSize = size;\r\n      this.refresh();\r\n    },\r\n\r\n    handleSkip(id) {\r\n      this.$router.push({\r\n        path: '/book',\r\n        query: { id }\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.result-grid {\r\n  margin: 20px 0;\r\n}\r\n\r\n.price-rating {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-top: 8px;\r\n}\r\n\r\n.rating-box {\r\n  background: #f5f7fa;\r\n  border-radius: 4px;\r\n  padding: 2px 6px;\r\n  font-size: 12px;\r\n  color: #606266;\r\n}\r\n\r\n.book-title {\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  margin: 5px 0;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n  white-space: nowrap;\r\n}\r\n\r\n.book-author {\r\n  color: #909399;\r\n  font-size: 12px;\r\n  margin: 5px 0;\r\n}\r\n\r\n.book-press {\r\n  color: #909399;\r\n  font-size: 12px;\r\n  margin: 5px 0;\r\n}\r\n\r\n.price {\r\n  color: #ff4443;\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n}\r\n\r\n.image-placeholder {\r\n  height: 276px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: #f5f7fa;\r\n  color: #c0c4cc;\r\n}\r\n</style>"],"mappings":";AAyEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,KAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,KAAA;IACA,QAAAC,CAAA;MACA,KAAAF,OAAA;IACA;EACA;EACAG,OAAA;IACA,MAAAH,QAAA;MACA,KAAAN,OAAA;MACA;QACA,MAAAU,GAAA,cAAAC,MAAA,CAAAC,IAAA,MAAAC,QAAA;UACAV,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAU,KAAA;YACAC,OAAA,OAAAP,MAAA,CAAAQ,KAAA,CAAAD;UACA;QACA;QACA,IAAAL,GAAA,CAAAX,IAAA,CAAAkB,IAAA;UACA,KAAAhB,KAAA,GAAAS,GAAA,CAAAX,IAAA,EAAAA,IAAA;UACA,KAAAG,KAAA,GAAAQ,GAAA,CAAAX,IAAA,EAAAG,KAAA;UACAgB,OAAA,CAAAC,GAAA,CAAAT,GAAA;UACAQ,OAAA,CAAAC,GAAA,CAAAT,GAAA,CAAAX,IAAA,CAAAA,IAAA;QACA;MACA,SAAAqB,KAAA;QACA,KAAAC,QAAA,CAAAD,KAAA;MACA;QACA,KAAApB,OAAA;MACA;IACA;IACAsB,iBAAAC,IAAA;MACA,KAAApB,OAAA,GAAAoB,IAAA;MACA,KAAAjB,OAAA;IACA;IACAkB,iBAAAC,IAAA;MACA,KAAArB,QAAA,GAAAqB,IAAA;MACA,KAAAnB,OAAA;IACA;IAEAoB,WAAAC,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QACAC,IAAA;QACAd,KAAA;UAAAW;QAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}