tal;\r\n        console.log(res)\r\n      } catch (error) {\r\n        this.$message.error('搜索失败，请稍后重试');\r\n      }\r\n    },\r\n    handlePageChange(page) {\r\n      this.pageNum = page;\r\n      this.refresh();\r\n    },\r\n    handleSizeChange(size) {\r\n      this.pageSize = size;\r\n      this.refresh();\r\n    },\r\n\r\n    handleSkip(id) {\r\n      this.$router.push({\r\n        path: '/book',\r\n        query: { id }\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AAmDA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,KAAA;IACA,QAAAC,CAAA;MACA,KAAAF,OAAA;IACA;EACA;EACAG,OAAA;IACA,MAAAH,QAAA;MACA,MAAAI,MAAA;QACAP,OAAA,OAAAA,OAAA;QACAC,QAAA,OAAAA,QAAA;QACAO,KAAA;UACAC,OAAA,OAAAJ,MAAA,CAAAK,KAAA,CAAAD;QACA;MACA;MAEA;QACA,MAAAE,GAAA,cAAAC,MAAA,CAAAC,IAAA,MAAAC,QAAA,mBAAAP,MAAA;QACA,KAAAT,KAAA,GAAAa,GAAA,CAAAd,IAAA,CAAAkB,OAAA;QACA,KAAAhB,KAAA,GAAAY,GAAA,CAAAd,IAAA,CAAAE,KAAA;QACAiB,OAAA,CAAAC,GAAA,CAAAN,GAAA;MACA,SAAAO,KAAA;QACA,KAAAC,QAAA,CAAAD,KAAA;MACA;IACA;IACAE,iBAAAC,IAAA;MACA,KAAArB,OAAA,GAAAqB,IAAA;MACA,KAAAlB,OAAA;IACA;IACAmB,iBAAAC,IAAA;MACA,KAAAtB,QAAA,GAAAsB,IAAA;MACA,KAAApB,OAAA;IACA;IAEAqB,WAAAC,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QACAC,IAAA;QACAlB,KAAA;UAAAe;QAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}