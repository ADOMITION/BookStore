      await this.$router.push('/login');\n        } else {\n          this.$message.error(data.msg);\n        }\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["PasswordStrength","components","data","loading","form","no","name","password","checkPass","email","phone","company","rules","required","message","trigger","min","max","pattern","validator","r","v","c","Error","type","methods","submit","$refs","registerForm","validate","$axios","post","$httpUrl","code","$message","success","$router","push","error","msg"],"sources":["src/Register.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <div class=\"login-box\">\r\n      <div class=\"animated fadeIn\">\r\n        <h1 class=\"login-title\">用户注册</h1>\r\n        <el-form\r\n            ref=\"registerForm\"\r\n            :model=\"form\"\r\n            :rules=\"rules\"\r\n            @keyup.enter.native=\"submit\"\r\n        >\r\n          <!-- 账号 -->\r\n          <el-form-item prop=\"no\">\r\n            <el-input\r\n                v-model=\"form.no\"\r\n                prefix-icon=\"el-icon-user\"\r\n                placeholder=\"请输入账号（3-16位字母数字）\"\r\n                clearable\r\n            ></el-input>\r\n          </el-form-item>\r\n\r\n          <!-- 姓名 -->\r\n          <el-form-item prop=\"name\">\r\n            <el-input\r\n                v-model=\"form.name\"\r\n                prefix-icon=\"el-icon-user-solid\"\r\n                placeholder=\"请输入真实姓名\"\r\n                clearable\r\n            ></el-input>\r\n          </el-form-item>\r\n\r\n          <!-- 密码 -->\r\n          <el-form-item prop=\"password\">\r\n            <el-input\r\n                v-model=\"form.password\"\r\n                prefix-icon=\"el-icon-lock\"\r\n                placeholder=\"至少8位，包含字母和数字\"\r\n                show-password\r\n                clearable\r\n            ></el-input>\r\n            <password-strength :password=\"form.password\" />