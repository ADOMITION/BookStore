{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nexport default {\n  name: \"NavBar\",\n  computed: {\n    processedData() {\n      return this.bean.map(category => {\n        return {\n          ...category,\n          books: category.books.map(book => ({\n            ...book,\n            // 后端返回的是10分制，保持原值\n            mark: book.mark || 0 // 处理空值情况\n          }))\n        };\n      });\n    }\n  },\n  data() {\n    return {\n      bean: [],\n      books: []\n    };\n  },\n  created() {\n    this.$axios.get(this.$httpUrl + '/book/preview').then(res => {\n      console.log(res);\n      this.bean = res.data;\n    });\n  },\n  methods: {\n    handleMore(id) {\n      this.$router.push({\n        path: '/category',\n        query: {\n          id: id\n        }\n      });\n    },\n    handleSkip(id) {\n      this.$router.push({\n        path: '/book',\n        query: {\n          id: id\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","computed","processedData","bean","map","category","books","book","mark","data","created","$axios","get","$httpUrl","then","res","console","log","methods","handleMore","id","$router","push","path","query","handleSkip"],"sources":["src/user/home/NvaBar.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-row v-for=\"row in bean\"\r\n            :key=\"row.id\"\r\n            style=\"text-align: center;margin-left: 20%;margin-right: 20%\"\r\n            :gutter=\"20\">\r\n      <el-col>\r\n        <h3>{{row.name}}\r\n          <el-button type=\"info\"\r\n                     icon=\"el-icon-arrow-right\"\r\n                     size=\"mini\"\r\n                     @click=\"handleMore(row.id)\"/>\r\n        </h3>\r\n      </el-col>\r\n      <el-col v-for=\"item in row.books\"\r\n              :key=\"item.id\"\r\n              :span=\"6\"\r\n      >\r\n        <el-card :body-style=\"{padding:'0px'}\"\r\n                 style=\"width: 195px\"\r\n                 shadow=\"hover\"\r\n                 @click.native=\"handleSkip(item.id)\">\r\n          <el-image :src=\"item.image\"\r\n                    style=\"height: 276px;width: 195px\"\r\n          />\r\n          <div style=\"text-align: center\">\r\n            <div class=\"rating-container\">\r\n              <el-rate\r\n                  v-model=\"item.mark\"\r\n                  :max=\"10\"\r\n                  :colors=\"['#99A9BF', '#F7BA2A', '#FF9900']\"\r\n                  disabled\r\n                  show-score\r\n                  score-template=\"{value} 分\">\r\n              </el-rate>\r\n            </div>\r\n            <p style=\"font-size: 5px\">{{item.name}}</p>\r\n            <p style=\"color: #9d9d9d;font-size: 3px\">{{item.author}}</p>\r\n            <p style=\"color:#fb4e44;font-size: 5px\">¥{{item.price}}</p>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"NavBar\",\r\n  computed: {\r\n    processedData() {\r\n      return this.bean.map(category => {\r\n        return {\r\n          ...category,\r\n          books: category.books.map(book => ({\r\n            ...book,\r\n            // 后端返回的是10分制，保持原值\r\n            mark: book.mark || 0  // 处理空值情况\r\n          }))\r\n        }\r\n      })\r\n    }\r\n  },\r\n  data(){\r\n    return{\r\n      bean:[],\r\n      books:[],\r\n    }\r\n  },\r\n  created() {\r\n    this.$axios.get(this.$httpUrl+'/book/preview').then(res=>{\r\n      console.log(res)\r\n      this.bean = res.data;\r\n    })\r\n  },\r\n\r\n  methods:{\r\n    handleMore(id){\r\n      this.$router.push({\r\n        path: '/category',\r\n        query:{\r\n          id:id\r\n        }\r\n      })\r\n    },\r\n    handleSkip(id){\r\n      this.$router.push({\r\n        path: '/book',\r\n        query:{\r\n          id:id\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.rating-container {\r\n  margin: 5px 0;\r\n  transform: scale(0.8); /* 调整评分组件大小 */\r\n}\r\n\r\n/* 调整评分文本样式 */\r\n.el-rate__text {\r\n  font-size: 12px!important;\r\n  color: #ff9900!important;\r\n  margin-left: 5px;\r\n}\r\n</style>"],"mappings":";;;AA+CA;EACAA,IAAA;EACAC,QAAA;IACAC,cAAA;MACA,YAAAC,IAAA,CAAAC,GAAA,CAAAC,QAAA;QACA;UACA,GAAAA,QAAA;UACAC,KAAA,EAAAD,QAAA,CAAAC,KAAA,CAAAF,GAAA,CAAAG,IAAA;YACA,GAAAA,IAAA;YACA;YACAC,IAAA,EAAAD,IAAA,CAAAC,IAAA;UACA;QACA;MACA;IACA;EACA;EACAC,KAAA;IACA;MACAN,IAAA;MACAG,KAAA;IACA;EACA;EACAI,QAAA;IACA,KAAAC,MAAA,CAAAC,GAAA,MAAAC,QAAA,oBAAAC,IAAA,CAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA,KAAAZ,IAAA,GAAAY,GAAA,CAAAN,IAAA;IACA;EACA;EAEAS,OAAA;IACAC,WAAAC,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QACAC,IAAA;QACAC,KAAA;UACAJ,EAAA,EAAAA;QACA;MACA;IACA;IACAK,WAAAL,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QACAC,IAAA;QACAC,KAAA;UACAJ,EAAA,EAAAA;QACA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}